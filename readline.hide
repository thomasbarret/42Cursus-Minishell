{
   ignore_libreadline_leaks
   Memcheck:Leak
   ...
   obj:*/libreadline.so.*
}
{
    _readline_
        Memcheck:Leak
        match-leak-kinds: all
        ...
        fun:readline
}
{
    _add_history_
        Memcheck:Leak
        match-leak-kinds: all
        ...
        fun:add_history
}  
{
    bin
    Memcheck:Leak
    ...
    obj:/usr/*
}
{
    bin2
    Memcheck:Leak
    ...
    obj:/usr/bin/*
}